generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Conversion {
  id            BigInt   @id @default(autoincrement())
  filename      String
  originalName  String
  filetype      String
  fileSize      Int
  pdfFilename   String?
  userEmail     String?
  uploadTime    DateTime @default(now())
  status        String

  @@index([status])
  @@index([uploadTime])
}